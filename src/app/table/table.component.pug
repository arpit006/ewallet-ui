div
  mat-card.mat-card.test
    mat-card-header.mat-card-header.header
      button.button(mat-raised-button) DELETE
    mat-card-content.mat-card-content
      .table-container
        mat-table.mat-table(#table [dataSource]='tableDataSource', matSort)
          ng-container(*ngFor='let columns of displayedColumns', matColumnDef= '{{columns}}')
            mat-header-cell.heading(*matHeaderCellDef, mat-sort-header) {{getTableHeader(columns)}}
            mat-cell(*matCellDef='let row') {{row[columns]}}
          mat-header-row(*matHeaderRowDef='displayedColumns;')
          mat-row(*matRowDef='let row; columns:displayedColumns')
        mat-divider
        mat-paginator([pageSizeOptions]= [5,10,20,30], showFirstLastButtons)
